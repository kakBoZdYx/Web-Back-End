    <!-- ===== ===== Main-Container ===== ===== -->
    <div class="container">
        <!-- ===== ===== Header===== ===== -->


        <!-- ===== ===== User Main-Profile ===== ===== -->
        <section class="userProfile card">
            <div class="profile">
                <figure><img src="site/img/alm.png" alt="profile" width="250px" height="250px"></figure>
            </div>
        </section>


        <!-- ===== ===== Work & Skills Section ===== ===== -->
        <section class="work_skills card">

            <!-- ===== ===== Work Contaienr ===== ===== -->

            <!-- ===== ===== Skills Contaienr ===== ===== -->
            <div class="skills">
                <h1 class="heading">Goals</h1>
                <ul>
                    <li style="--i:0">Gain 5 kg</li>
                    <li style="--i:1">Maintain healthy lifestyle</li>
                    <li style="--i:2">Improve mental health</li>
                    <li style="--i:3">
                      Improve body quality
                    </li>
                </ul>
            </div>
        </section>


        <!-- ===== ===== User Details Sections ===== ===== -->
        <section class="userDetails card">
            <div class="userName">
                <h1 class="name">Almira </h1>
                <div class="map">
                    <i class="ri-map-pin-fill ri"></i>
                    <span>Astana, KZ</span>
                </div>
                <p></p>
            </div>

            <div class="rank">
                <h1 class="heading">Healthy days</h1>
                <span>25</span>
                <div class="rating">
                    <i class="ri-star-fill rate"></i>
                    <i class="ri-star-fill rate"></i>
                    <i class="ri-star-fill rate"></i>
                    <i class="ri-star-fill rate"></i>
                    <i class="ri-star-fill underrate"></i>
                </div>
            </div>

            <div class="btns">
                <ul>
                    <li class="sendMsg">
                        <i class="ri-chat-4-fill ri"></i>
                        <a href="#">Achievements</a>
                    </li>

                    <li class="sendMsg active">
                        <i class="ri-check-fill ri"></i>
                        <a href="#">Tracker</a>
                    </li>

                    <li class="sendMsg">
                        <a href="#">Workouts</a>
                    </li>
                </ul>
            </div>
        </section>


        <!-- ===== ===== Timeline & About Sections ===== ===== -->
        <section class="timeline_about card">
            <div class="tabs">
                <ul>
                    <li class="timeline">
                        <i class="ri-eye-fill ri"></i>
                        <span>Timeline</span>
                    </li>

                    <li class="about active">
                        <i class="ri-user-3-fill ri"></i>
                        <span>About</span>
                    </li>
                </ul>
            </div>

            <div class="contact_info">
                <h1 class="heading">Contact Information</h1>
                <ul>
                    <li class="phone">
                        <h1 class="label">e-mail:</h1>
                        <span class="info">211376@astanait.edu.kz</span>
                    </li>



                </ul>
            </div>

            <div class="basic_info">
                <h1 class="heading">Basic Information</h1>
                <ul>
                    <li class="birthday">
                        <h1 class="label">Birthday:</h1>
                        <span class="info">Apr 1, 2004</span>
                    </li>

                    <li class="sex">
                        <h1 class="label">Gender:</h1>
                        <span class="info">Female</span>
                    </li>
                </ul>
            </div>
        </section>
    </div>
